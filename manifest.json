{
    "manifest_version": 2,
    "name": "我的IP",
    "version": "1.0",
    "description": "查找我的IP",
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "我的IP",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://120.27.4.87:888/getip"
    ],
    "background": {
        "scripts": ["js/status.js"]
    }
}
